#nop diebao---------------------------------------------------------------------------------;
#act {^你向余d打听有关『job』的消息。} {
    #class _class_diebao kill;
    #var _job db;

    #class _class_diebao open;

    #act {^余d在你的耳边悄声说道：为了保护接头人的安全，接头人的名字不能透露，我只能告诉你某人在%+等着你。} {
        #var job_loc {%%1};
    } 3;

    #act {^长相大致如下%+} {
        #var _job_diebao_look {%%1};
    } 3;

    #nop CAUTION: in action, variable name beginning with underscore: ${_variable_name};
    #nop CAUTION: watch out for empty variable match?;
    #var _job_diebao_look {防止误匹fdjkajfdklajfdksl};
    #act {${_job_diebao_look}} {
        ask $_look_id about 接头;

    } 3;

    #act {^你向%+打听有关『接头』的消息。} {
        #act {^%%1在你的耳边悄声说道：有人在%+给你留下了下一步行动的步骤。} {
            #var job_loc {%%%1};
        } 3;
    } 3;

    #act {^[%+]%s$myName钧鉴：} {
        read %%1;
    } 3;

    #act {^----------------------------------------------------------------------} {
        #line oneshot #act {^%+$} {
            #var _db_code {%%%1};
            #repl _db_code {，暗号是} {|};
            #repl _db_code {，暗号} {|};
            #repl _db_code {，说出} {|};

            #repl _db_code {，请找} {|};
            #repl _db_code {其人名曰} {|};
            #repl _db_code {，名唤} {|};
            #repl _db_code {会找你。} {|};
            #repl _db_code {正在%+等着你} {|&1};

            #repl _db_code {{，|　|||＿| |“|”|。|#|◎|★|O|}|〓}} {|};

            #repl _db_code {{\|\|+}} {|};
            #repl _db_code {%+|$} {&1};
            #echo {db code: $_db_code};

            #var job_name {$_db_code};
        } 1;
    } 3;

    #nop act {^%+，暗号%+，请找%+} {
        #var job_loc {%%1};
        #var _db_code {%%2};
        #var job_name {%%3};
    } 3;

    #act {^{(?!【)\S+}见状脚下抹油，“你掩护，我先撤！”，别忘了告之右丞。} {
        #class _class_diebao open;
        #act {^    大宋 右丞相兼枢密使 余d(Yu jie)} {
            t-ticker;
            ask yu about finish;
        } 3;
        #class _class_diebao close;
    } 3;

    #act {{^   \( \|     完成谍报任务之后，你被奖励了：|^你向余d打听有关『fail』的消息。}} {
        #class _class_diebao kill;
        t@ticker;
        #var job_loc -;
        #var job_name -;
    } 3;

    #class _class_diebao close;
} 3;

